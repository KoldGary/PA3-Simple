# LONN: List of Non-empty Numbers
# Lexical specification

skip WHITESPACE '\s+'
skip COMMENT '#.*'
token LP '\('
token RP '\)'
token NUM '\d+'
%
# Syntactic specification
<lonn>:Program ::= LP <nums> RP
<nums> ::= <num> <rest>
<rest>:NumList ::= <nums>
<rest>:Empty ::= 
<num> ::= <NUM>
%
# Semantic specification
# Students will implement the minimum-value semantics here